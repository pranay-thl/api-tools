{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./dist",
      "removeComments": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "sourceMap": false
    },
    "include": ["src/**/*", "src/**/*.json"],
  }